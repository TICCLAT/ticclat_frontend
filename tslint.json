{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ],
    "rules": {
        "prettier": true,
        "interface-name": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
        ]
    }
}