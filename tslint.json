{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "interface-name": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "quotemark": [true, "single", "jsx-double"],
        "ordered-imports": false,
        "member-access": [true, "no-public"],
        "jsx-no-multiline-js": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
            "src/comp[onents/timeline.tsx"
        ]
    }
}
