{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "arrow-parens": false,
        "interface-name": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false,
        "member-access": [true, "no-public"],
        "no-console": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "ordered-imports": false,
        "quotemark": [true, "single", "jsx-double"]
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}
