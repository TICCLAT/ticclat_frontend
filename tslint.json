{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "rules": {
        "interface-name": false,
        "no-submodule-imports": false,
        "member-access": false,
        "ordered-imports": false,
        "member-ordering": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "object-literal-sort-keys": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
            "src/components/timeline.tsx"
        ]
    }
}
